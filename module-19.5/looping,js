/**
-- Looping Technique
-- 1. for Looping
-- 2. while Looping
-- 3. do while -- > ignore
-- 4. for of --> array loop korar jonno
-- 5. for in --> object loop korar jonno

-- একটা থেকে প্রতিটি উপাদান আলাদা আলাদা করে পেতে
*/

const friends = ['Elon', 'Bill', 'Mark', 'Waren'];

for(const friend of friends){
    console.log(friend);
}